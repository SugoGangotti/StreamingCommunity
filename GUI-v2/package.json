{
  "name": "streamingcommunity-gui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview --open",
    "backend:install": "cd backend && pip install -r requirements.txt",
    "backend:migrate": "cd backend && python manage.py migrate",
    "backend:dev": "cd backend && python manage.py runserver 0.0.0.0:8000",
    "backend:setup": "npm run backend:install && npm run backend:migrate",
    "dev:full": "concurrently \"npm run backend:dev\" \"npm run dev\"",
    "install:all": "npm install && npm run backend:install"
  },
  "dependencies": {
    "lucide-react": "^0.548.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^6.27.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.4.5",
    "vite": "^5.4.8"
  }
}
